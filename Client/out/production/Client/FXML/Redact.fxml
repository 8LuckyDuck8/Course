<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="Window" prefHeight="400.0" prefWidth="600.0" stylesheets="@../CSS/Back.css" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.RedactUser">
   <children>
      <Label layoutX="121.0" layoutY="97.0" text="Ввдите новый пароль для пользователя" textFill="WHITE">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
         <effect>
            <DropShadow />
         </effect>
      </Label>
      <Label layoutX="176.0" layoutY="176.0" text="Пароль:" textFill="WHITE">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
         <effect>
            <DropShadow />
         </effect>
      </Label>
      <TextField fx:id="newPassword" layoutX="226.0" layoutY="172.0" onAction="#createNewPassword" promptText="Введите пароль" />
      <Button layoutX="169.0" layoutY="286.0" mnemonicParsing="false" onAction="#createNewPassword" prefWidth="110.0" text="Редактировать">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <Button layoutX="319.0" layoutY="286.0" mnemonicParsing="false" onAction="#BackButton" prefWidth="110.0" text="Назад">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
